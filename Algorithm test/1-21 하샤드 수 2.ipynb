   ],
   "source": [
    "def solution(n):\n",
    "    answer = []\n",
    "    for c in str(n):\n",
    "        answer.append(int(c))\n",
    "    return n % sum(answer) == 0\n",
    "    \n",
    "solution(11)"
   ]
