     "name": "stdout",
     "output_type": "stream",
     "text": [
      "i sum\n",
      "1 1\n",
      "2 3\n",
      "3 6\n",
      "4 10\n",
      "5 15\n",
      "6 21\n"
     ]
    }
   ],
   "source": [
    "i = 0 # 초기화 => 숫자 계산을 하겠다는 뜻\n",
    "sum = 0 # 초기화 => 숫자 계산을 하겠다는 뜻\n",
    "\n",
    "print(\"i sum\")\n",
    "while (sum < 20): # 조건 검사\n",
    "    i = i + 1 # i를 1씩 증가\n",
    "    sum = sum + i # 이전의 sum과 현재 i를 더해서 sum을 업데이트\n",
    "    print(i, sum)"
   ]
  },
